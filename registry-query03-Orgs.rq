# ?org ?ds_title 
SELECT ?org ?org_name (COUNT(?ds) AS ?ds_count_by_org) {
    ?org a org:Organization ;
         foaf:name ?org_name .

    ?ds a dcat:DataService ;
        dct:title ?ds_title .      
 }
GROUP BY ?org ?org_name


