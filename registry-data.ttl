# 3. DATA SERVICE (Tool) with CPP links
<https://eden-fidelis.eu/example/registry/service_4>
    rdf:type          dcat:DataService ;
    rdfs:label        "File Validation Service (DPS-FI)"@en ;
    dct:title         "File Validation Service for Finnish National Digital Preservation Services"@en ;
    dct:description   """You can validate your files against the National Digital Preservation Services (DPS) in Finland using this online service. The service displays the validity and support in the DPS for the files being validated."""@en ;
    dcat:endpointURL  <https://validation.digitalpreservation.fi/#/en> ;
    dcat:landingPage  <https://digitalpreservation.fi/> ;
    dct:publisher     <https://eden-fidelis.eu/example/registry/org_2> ;  # Link to the CSC organization
    dcat:contactPoint <https://eden-fidelis.eu/example/registry/contact_2> ; # Link to the CSC contact
    foaf:page         <https://github.com/Digital-Preservation-Finland/file-scraper> ; # Link to documentation/source code
    cpp:has_cpp       cpp-doc:001, cpp-doc:002 . # Link to CPPs

