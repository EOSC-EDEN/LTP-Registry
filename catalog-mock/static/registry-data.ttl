PREFIX adms:  <http://www.w3.org/ns/adms#>
PREFIX dcat:  <http://www.w3.org/ns/dcat#>
PREFIX dct:   <http://purl.org/dc/terms/>
PREFIX foaf:  <http://xmlns.com/foaf/0.1/>
PREFIX org:   <http://www.w3.org/ns/org#>
PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos:  <http://www.w3.org/2004/02/skos/core#>
PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>
PREFIX edenr: <https://eden-fidelis.eu/example/registry/>
PREFIX geon: <http://www.geonames.org/ontology#>


<https://eden-fidelis.eu/example/registry/service_1>
    rdf:type       dcat:DataService;
    rdfs:label     "DANS Data Vault Catalog"@en;
    dcat:endpointURL  <https://catalog.vault.datastations.nl>;
    dct:title      "DANS Data Vault Catalog"@en ;
    dcat:contactPoint <https://eden-fidelis.eu/example/registry/contact_1> ;
    dct:description "A safe and sustainable long-term preservation archive that meets the highest archiving requirements. The DANS Data Vault contains all datasets that have been entrusted to DANS, including the data from the Data Stations and DataverseNL."@en ;
    dcat:landingPage <https://catalog.vault.datastations.nl> ;
    dct:publisher <https://eden-fidelis.eu/example/registry/DANS> ;
    dcat:keyword "preservation"@en, "archive"@en, "research data"@en ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/TECH> ;
    dct:conformsTo <https://www.w3.org/TR/vocab-dcat-2/> ;
    dct:accessRights <http://publications.europa.eu/resource/authority/access-right/PUBLIC> .

<https://eden-fidelis.eu/example/registry/service_2>
    rdf:type       dcat:DataService;
    rdfs:label     "European Open Science Cloud Portal"@en;
    dcat:endpointURL  <https://eosc-portal.eu/api>;
    dct:title      "EOSC Portal API"@en ;
    dcat:contactPoint <https://eden-fidelis.eu/example/registry/contact_2> ;
    dct:description "The European Open Science Cloud (EOSC) Portal provides access to a broad spectrum of resources for research."@en ;
    dcat:landingPage <https://eosc-portal.eu> ;
    dct:publisher <https://eden-fidelis.eu/example/registry/EOSC> ;
    dcat:keyword "open science"@en, "cloud"@en, "european"@en ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/TECH>, <http://publications.europa.eu/resource/authority/data-theme/EDUC> ;
    dct:conformsTo <https://www.w3.org/TR/vocab-dcat-2/> ;
    dct:accessRights <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> .

<https://eden-fidelis.eu/example/registry/service_3>
    rdf:type       dcat:DataService;
    rdfs:label     "Zenodo Research Repository"@en;
    dcat:endpointURL  <https://zenodo.org/api>;
    dct:title      "Zenodo API"@en ;
    dcat:contactPoint <https://eden-fidelis.eu/example/registry/contact_3> ;
    dct:description "Zenodo is a general-purpose open-access repository developed under the European OpenAIRE program."@en ;
    dcat:landingPage <https://zenodo.org> ;
    dct:publisher <https://eden-fidelis.eu/example/registry/CERN> ;
    dcat:keyword "open access"@en, "repository"@en, "publications"@en ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/TECH> ;
    dct:conformsTo <https://www.w3.org/TR/vocab-dcat-2/>, <https://www.openarchives.org/OAI/openarchivesprotocol.html> ;
    dct:accessRights <http://publications.europa.eu/resource/authority/access-right/PUBLIC> .

<https://eden-fidelis.eu/example/registry/DANS>
    rdf:type org:Organization ;
    foaf:name "Data Archiving Networked Services (DANS)"@en ;
    org:identifier <https://ror.org/008pnp284> ;
    vcard:country-name "The Netherlands"@en ;
    geon:geonamesID <https://www.geonames.org/2747373> .

<https://eden-fidelis.eu/example/registry/EOSC>
    rdf:type org:Organization ;
    foaf:name "European Commission"@en ;
    org:identifier <https://ror.org/00k4n6c32> ;
    vcard:country-name "Belgium"@en ;
    geon:geonamesID <https://www.geonames.org/2800866> .

<https://eden-fidelis.eu/example/registry/CERN>
    rdf:type org:Organization ;
    foaf:name "CERN"@en ;
    org:identifier <https://ror.org/01ggx4157> ;
    vcard:country-name "Switzerland"@en ;
    geon:geonamesID <https://www.geonames.org/2658434> .

<https://eden-fidelis.eu/example/registry/contact_1>
    rdf:type  vcard:Kind ;
    vcard:fn  "Data Archiving Networked Services (DANS)"@en ;
    vcard:hasEmail <mailto:info@dans.knaw.nl> ;
    vcard:hasURL <https://dans.knaw.nl/> .

<https://eden-fidelis.eu/example/registry/contact_2>
    rdf:type  vcard:Kind ;
    vcard:fn  "EOSC Portal Support"@en ;
    vcard:hasEmail <mailto:support@eosc-portal.eu> ;
    vcard:hasURL <https://eosc-portal.eu/contact> .

<https://eden-fidelis.eu/example/registry/contact_3>
    rdf:type  vcard:Kind ;
    vcard:fn  "Zenodo Support"@en ;
    vcard:hasEmail <mailto:info@zenodo.org> ;
    vcard:hasURL <https://zenodo.org/support> .




